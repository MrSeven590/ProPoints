  const uIndex = str.indexOf('u')
  if (uIndex != -1) {
    return str.replace('u', 'v')
  }
  return str.replace('v', 'u')
}

function addPinyinEntry(pinyin: string, chars: string): void {
  _allPinyin.push(pinyin)
  _allPinyinSet.add(pinyin)

  for (let i = 0; i < chars.length; i++) {
    const ch = chars.charAt(i)
    const existing = _charToPinyin.get(ch)
    if (existing == null) {
      _charToPinyin.set(ch, pinyin)
    } else {
      _charToPinyin.set(ch, existing + ' ' + pinyin)
    }
  }
}

function ensureInitialized(): void {
  if (_initialized) {
    return
  }

  SIMPLIFIED_DICT.forEach(function (chars: string, pinyin: string) {
    addPinyinEntry(pinyin, chars)

    if (UV_LIST.indexOf(pinyin) != -1) {
      addPinyinEntry(replaceUv(pinyin), chars)
    }

    if (V_KEY_LIST.indexOf(pinyin) != -1) {
      addPinyinEntry(pinyin.replace('v', 'ü'), chars)
    }
  })

  _initialized = true
}

function normalizeKey(keys: string): string {
  let normalized = ''
  const lower = keys.toLowerCase()

  for (let i = 0; i < lower.length; i++) {
    const ch = lower.charAt(i)
    if (ch == ' ' || ch == '\t' || ch == '\n' || ch == '\r') {
      continue
    }

    const mapped = DIACRITIC_TABLE.get(ch)
    if (mapped != null) {
      normalized += mapped
    } else {
      normalized += ch
    }
  }

  return normalized
}

function normalizeText(text: string): string {
  let normalized = ''
  const lower = text.toLowerCase()

  for (let i = 0; i < lower.length; i++) {
    const ch = lower.charAt(i)
    // keep spaces in input so we can handle match across spaces later
    if (ch == ' ') {
      normalized += ch
      continue
    }

    const mapped = DIACRITIC_TABLE.get(ch)
    if (mapped != null) {
      normalized += mapped
    } else {
      normalized += ch
    }
  }

  return normalized
}

function getPinyinArray(text: string): string[] {
  ensureInitialized()

  const result: string[] = []
  for (let i = 0; i < text.length; i++) {
    const ch = text.charAt(i)
    const py = _charToPinyin.get(ch)
    if (py != null) {
      result.push(py)
    } else {
      result.push(ch)
    }
  }
  return result
}

function hasAnyPinyinPrefix(prefix: string): boolean {
  for (let i = 0; i < _allPinyin.length; i++) {
    if (_allPinyin[i].indexOf(prefix) == 0) {
      return true
    }
  }
  return false
}

function listPinyinWithPrefix(prefix: string): string[] {
  const result: string[] = []
  for (let i = 0; i < _allPinyin.length; i++) {
    const py = _allPinyin[i]
    if (py.indexOf(prefix) == 0) {
      result.push(py)
    }
  }
  return result
}

// 拼音切分：将关键词拆分为可能的拼音片段组合
function wordBreak(s: string): string[] {
  ensureInitialized()

  const solutions: string[] = []
  const possible: boolean[] = []
  for (let i = 0; i <= s.length; i++) {
    possible.push(true)
  }

  const current: WordPiece[] = []
  getAllSolutions(0, s, current, solutions, possible)
  return solutions
}

function getAllSolutions(
  start: number,
  s: string,
  current: WordPiece[],
  solutions: string[],
  possible: boolean[]
): void {
  const len = s.length

  if (start == len) {
    solutions.push(current.join(' '))
    return
  }

  for (let i = start; i < len; i++) {
    const piece = s.substring(start, i + 1)
    let isMatch = false

    // 最后一个音节特殊处理：允许不输入完整拼音，只要是某个拼音的前缀即可
    const isLastChar = (i + 1 == len)
    if (isLastChar && possible[i + 1] && hasAnyPinyinPrefix(piece)) {
      if (piece.length == 1) {
        current.push(piece)
      } else {
        current.push(listPinyinWithPrefix(piece))
      }
      isMatch = true
    } else {
      if (possible[i + 1] && _allPinyinSet.has(piece)) {
        current.push(piece)
        isMatch = true
      }
    }

    if (isMatch) {
      const before = solutions.length
      getAllSolutions(i + 1, s, current, solutions, possible)
      if (solutions.length == before) {
        possible[i + 1] = false
      }
      current.pop()
    }
  }
}

function getFullKey(key: string): string[][] {
  const result: string[][] = []

  const solutions = wordBreak(key)
  for (let i = 0; i < solutions.length; i++) {
    const parts = solutions[i].split(' ')
    if (parts.length == 0) {
      continue
    }

    const lastIndex = parts.length - 1
    const lastPart = parts[lastIndex]
    const options = lastPart.split(',')

    for (let j = 0; j < options.length; j++) {
      const option = options[j]
      const one: string[] = []
      for (let k = 0; k < parts.length; k++) {
        if (k == lastIndex) {
          one.push(option)
        } else {
          one.push(parts[k])
        }
      }
      result.push(one)
    }
  }

  // Fallback: initials match (split into single letters)
  if (result.length == 0 || result[0].length != key.length) {
    result.push(key.split(''))
  }

  _keyCache.set(key, result)
  return result
}

function point2point(test: string, key: string, isLast: boolean, extend: boolean): boolean {
  if (test.length == 0) {
    return false
  }

  const parts: string[] = test.split(' ')

  if (extend) {
    const originalLen = parts.length
    for (let i = 0; i < originalLen; i++) {
      const item = parts[i]
      if (item.length > 0) {
        parts.push(item.charAt(0))
      }
    }
  }

  if (!isLast) {
    return parts.indexOf(key) != -1
  }

  for (let i = 0; i < parts.length; i++) {
    if (parts[i].indexOf(key) == 0) {
      return true
    }
  }

  return false
}

function getIndex(py: string[], fullString: string[][], keys: string): boolean {
  for (let p = 0; p < py.length; p++) {
    for (let k = 0; k < fullString.length; k++) {
      const keyArr = fullString[k]
      const keyLength = keyArr.length
      const extend = (keyLength == keys.length)

      if (keyLength > py.length) {
        continue
      }

      let isMatch = true
      let i = 0
      let preSpaceNum = 0
      let spaceNum = 0

      for (; i < keyArr.length; i++) {
        const preIndex = p + i + preSpaceNum
        if (i == 0 && preIndex < py.length && py[preIndex] == ' ') {
          preSpaceNum += 1
          i -= 1
          continue
        }

        const curIndex = p + i + spaceNum
        if (curIndex < py.length && py[curIndex] == ' ') {
          spaceNum += 1
          i -= 1
          continue
        }

        const testIndex = p + i + spaceNum
        if (testIndex >= py.length) {
          isMatch = false
          break
        }

        const hasNext = (p + i + 1 < py.length) && (i + 1 < keyArr.length)
        const isLast = !hasNext

        if (!point2point(py[testIndex], keyArr[i], isLast, extend)) {
          isMatch = false
          break
        }
      }

      if (isMatch) {
        return true
      }
    }
  }

  return false
}

/**
 * 检查 input 是否匹配 keyword
 */
export function pinyinMatch(input: string, keyword: string): boolean {
  const key = normalizeKey(keyword)
  if (key.length == 0) {
    return true
  }

  const normalizedInput = normalizeText(input)

  // Direct substring match
  if (normalizedInput.indexOf(key) != -1) {
    return true
  }

  // 原文匹配（跨空格匹配，行为对齐 pinyin-match 的 noPyIndex 步骤）
  const rawMatchKeys: string[][] = [key.split('')]
  if (getIndex(normalizedInput.split(''), rawMatchKeys, key)) {
    return true
  }

  // 拼音匹配
  const py = getPinyinArray(normalizedInput)
  const cached = _keyCache.get(key)
  const fullKey = cached != null ? cached : getFullKey(key)
  return getIndex(py, fullKey, key)
}

/**
 * 获取中文字符串的拼音首字母
 *
 * 示例：
 * - "张三" -> "zs"
 */
export function getInitials(cn: string): string {
  ensureInitialized()

  let result = ''
  const normalized = normalizeText(cn)

  for (let i = 0; i < normalized.length; i++) {
    const ch = normalized.charAt(i)

    if (ch == ' ' || ch == '\t' || ch == '\n' || ch == '\r') {
      continue
    }

    const py = _charToPinyin.get(ch)
    if (py != null) {
      // 取第一个拼音，保证首字母结果稳定
      const first = py.split(' ')[0]
      if (first.length > 0) {
        result += first.charAt(0)
      }
    } else {
      result += ch
    }
  }

  return result
}
